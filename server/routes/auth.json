{
    "middleware": [],
    "path": "",
    "routes": [
        {
            "path": "/auth/login/:module",
            "method": "get",
            "handler": "login",
            "middleware": []
        },
        {
            "path": "/auth/logout",
            "method": "get",
            "handler": "logout"
        },
        {
            "path": "/auth/:module/callback",
            "method": "get",
            "handler": "oauthCallback",
            "middleware": []
        },
        {
            "path": "/auth/validate/:code",
            "method": "get",
            "handler": "validate"
        },
        {
            "path": "/api/auth/validation/resend",
            "method": "post",
            "handler": "resendCode"
        }
    ]
}